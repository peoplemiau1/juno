struct Point {
    x
    y
}

fn main() {
    print("Struct Test Start")

    let p = Point
    p.x = 10
    p.y = 20

    print("Point initialized.")
    let sum = p.x + p.y
    print("Sum of fields:")
    print(sum)

    if (sum == 30) {
        print("Struct check SUCCESS")
        return 0
    }
    return 1
}
