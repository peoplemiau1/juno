struct Point {
    let x
    let y
}

fn main() {
    print("Struct Test Start")

    let p = Point
    print("Allocated")

    p.x = 10
    print("Assigned x")

    p.y = 20
    print("Assigned y")

    print("Point initialized.")
    print("p.x is:")
    print(p.x)

    let sum = p.x + p.y
    print("Sum of fields:")
    print(sum)

    if (sum == 30) {
        print("Struct check SUCCESS")
        return 0
    } else {
        print("Struct check FAILED")
        return 1
    }
}
