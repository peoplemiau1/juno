struct Entity {
    id
    active
}

fn Entity.init(id_val) {
    self.id = id_val
    self.active = 1
}

fn Entity.check() {
    return self.active
}

fn main() {
    print("--- JUNO FULL TEST ---")
    
    // 1. Математика
    let a = 10
    let b = 2
    let res = a * b + 5
    print("Math check (25):")
    print(res)
    
    // 2. Структуры и Методы
    let e = Entity
    e.init(7)
    
    print("Entity ID (7):")
    print(e.id)
    
    // 3. Условия
    let is_ok = e.check()
    if (is_ok) {
        print("Condition: Success")
    } else {
        print("Condition: Failed")
    }
    
    print("--- TEST COMPLETE ---")
    return 0
}
