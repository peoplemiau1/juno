struct Math {
    factor
}

fn Math.add(a, b) {
    let f = self.factor
    let res = a + b
    let val = res + f
    return val
}

fn main() {
    let m = Math
    m.factor = 10
    print(m.factor)
    
    let val = m.add(2, 3)
    print(val)
    if (val == 15) {
        return 0
    }
    return 1
}
