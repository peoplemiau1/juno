// test_stdlib.juno - Test standard library import

import "../stdlib/std.juno"

fn main() {
    // Math tests
    let sq = square(7)       // 49
    let f5 = factorial(5)    // 120
    let fb = fib(10)         // 55
    let g = gcd(48, 18)      // 6

    // Pair test
    let p = Pair
    p.init(10, 20)
    p.swap()
    let pf = p.first         // 20

    // Result: 49 + 120 + 55 + 6 + 20 = 250
    let res = sq + f5 + fb + g + pf
    if (res == 250) {
        return 0
    }
    return 1
}
